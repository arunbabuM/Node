<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"  href="/crudStyle.css">
    <title>Document</title>
</head>
<body>
    <h3>Student Management System</h3>
    <p  class="addbtn" ><a href="">Add User</a></p>
    <table>
        <thead>
            <tr>
                <td>#</td>
                <td>Name</td>
                <td>Age</td>
                <td>City</td>
                <td>Edit</td>
                <td>Delete</td>
            </tr>
        </thead>
    <tbody>
        <!-- <tr>
            <td>1</td>
            <td>Arun</td>
            <td>25</td>
            <td>Mumbai</td>
            <td><a href="" class="edit">Edit</a></td>
            <td><a href="" class="delete">Delete</a></td>
        </tr> -->

      <% for(const rows of data){ %>
      <tr>
        <td><%= rows.id %>&ensp;</td>
        <td><%= rows.name %>&ensp;</td>
        <td><%= rows.age %>&ensp;</td>
        <td><%= rows.city %>&ensp;</td>
        <td><a class="edit" <%= rows.ID %>&ensp;>Edit</a></td>
        <td><a class="delete" <%= rows.ID %>&ensp;>Delete</a></td>
      </tr>
      <%} %>
    </tbody>
    </table>
</body>
</html>
